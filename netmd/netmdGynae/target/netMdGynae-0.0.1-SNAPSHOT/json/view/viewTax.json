{
    "name": "viewTax",
    "id": "viewTax",
    "content": [
        {
            "className": "sep-div-view",
            "data": [
                {
                    "className": "column-left",
                  "data": [
                        {
                            "className": "column-one-fourth",
                            "elements": [
                                {
                                    "name": "id",
                                    "id": "id",
                                    "type": "newlabel",
                                    "style": "margin-left:6%",
                                    "title": "Id"
                                }
                            ]
                        },
                        {
                            "className": "column-three-fourth",
                            "elements": [
                                {
                                    "name": "name",
                                    "id": "name",
                                    "type": "text",
                                    "readonly": "readonly",
                                    "className": "newBox large",
                                    "title": "Name"
                                }
                            ]
                        }
                    ]
                },
                {
                    "className": "column-right",
                    "data": [
                        {
                            "className": "column-third ",
                            "elements": [
                                {
                                    "name": "value",
                                    "id": "value",
                                    "type": "text",
									"readonly": "readonly",
                                    "className": "newBox large",
                                    "title": "Value"
                                }
                            ]
                        },
						{
                                    "className": "column-last",
                                    "id": "viewType",
                                    "elements": [
                                        {
                                            "name": "lblViewCalculationType",
                                            "id": "lblViewCalculationType",
                                            "type": "newlabel",
                                            "title": "Calculation-Type"
                                        }
                                    ]
                                },
                        {
                            "className": "column-last",
							 "style": "display:none;",
                              "id": "editType",
                            "elements": [
                                {
                                    "name": "calculationType",
                                    "id": "calculationType",
                                    "type": "radio",
                                    "title": "Calculation-Type",
                                    "value": [
                                        {
                                            "title": "Fixed",
                                            "value": "Fixed",
                                            "checked": "true"
                                        },
                                        {
                                            "title": "Percentage",
                                            "value": "Percentage"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
				     {
                    "className": "five_sixth",
                    "elements": [
                        {
                            "name": "description",
                            "id": "description",
                            "type": "textarea",
                            "readonly": "readonly",
                            "className": "newBox large",
                            "title": "Description"
                        }
                    ]
                }
            ]
        },
        {
            "className": "sep-div-view",
            "style": "margin-bottom:5px",
            "data": [
                {
                    "className": "five_sixth",
                    "elements": [
                        {
                            "type": "button",
                            "content": {
                                "buttons": [
                                    {
                                        "name": "btnTaxEdit",
                                        "title": "Edit",
                                        "type": "button",
                                        "className": "stdbtn btn_black",
                                        "style": "opacity: 1; float:left;"
                                    },
                                    {
                                        "name": "btnTaxSave",
                                        "title": "Save",
                                        "type": "button",
                                        "className": "stdbtn btn_black",
                                        "style": "opacity:1;display:none;"
                                    },
                                    {
                                        "name": "btnTaxCancel",
                                        "title": "Cancel",
                                        "type": "button",
                                        "className": "stdbtn",
                                        "style": "opacity:1;display:none;"
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    ]
}