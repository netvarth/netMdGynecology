{
    "name": "viewRoomType",
    "id": "viewRoomType",
    "content": [
        {
            "className": "sep-div-view",
            "data": [
                {
                    "className": "column-left",
                    "data": [
                        {
                            "className": "column-one-fourth",
                            "elements": [
                                {
                                    "name": "id",
                                    "id": "id",
                                    "type": "newlabel",
                                    "style": "margin-left:6%",
                                    "title": "Id"
                                }
                            ]
                        },
                        {
                            "className": "column-three-fourth",
                            "elements": [
                                {
                                    "name": "type",
                                    "id": "type",
                                    "type": "text",
                                    "readonly": "readonly",
                                    "className": "newBox large",
                                    "title": "Type"
                                }
                            ]
                        }
                    ]
                },
                {
                    "className": "column-right",
                    "data": [
                        {
                            "className": "column-first",
                            "elements": [
                                {
                                    "name": "rent",
                                    "id": "rent",
                                    "type": "text",
                                    "readonly": "readonly",
                                    "className": "newBox large",
									"image": "/netmd/images/rupee-symbel.png",
                                    "title": "Rent"
                                }
                            ]
                        },
                        {
                            "className": "column-second",
                            "elements": [
                                {
                                    "name": "count",
                                    "id": "count",
                                    "type": "text",
                                    "readonly": "readonly",
                                    "className": "newBox large",
                                    "title": "No of Rooms"
                                }
                            ]
                        },
                        {
                            "className": "column-third",
                            "elements": [
                                {
                                    "name": "noOfBeds",
                                    "id": "noOfBeds",
                                    "type": "text",
                                    "readonly": "readonly",
                                    "className": "newBox large",
                                    "title": "Beds in a Room"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "className": "sep-div-view",
            "style": "margin-bottom:5px",
            "data": [
                {
                    "className": "five_sixth",
                    "elements": [
                        {
                            "type": "button",
                            "content": {
                                "buttons": [
                                    {
                                        "name": "btnRoomTypeEdit",
                                        "title": "Edit",
                                        "type": "button",
                                        "className": "stdbtn btn_black",
                                        "style": "opacity: 1; float:left;"
                                    },
                                    {
                                        "name": "btnRoomTypeSave",
                                        "title": "Save",
                                        "type": "button",
                                        "className": "stdbtn btn_black",
                                        "style": "opacity:1;display:none;"
                                    },
                                    {
                                        "name": "btnRoomTypeCancel",
                                        "title": "Cancel",
                                        "type": "button",
                                        "className": "stdbtn",
                                        "style": "opacity:1;display:none;"
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    ]
}