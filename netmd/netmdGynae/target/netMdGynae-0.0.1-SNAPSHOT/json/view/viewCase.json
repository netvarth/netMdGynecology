{
    "name": "viewCase",
    "id": "viewCase",
    "content": [
        {
            "className": "sep-div-view",
            "id": "generalCase",
            "data": [
                {
                    "className": "five_sixth",
                    "data": [
                        {
                            "className": "column-left",
                            "elements": [
                                {
                                    "name": "patientName",
                                    "id": "patientName",
                                    "type": "newlabel",
									"readonly":"readonly",
                                    "className": "large capital",
                                    "title": "PatientName"
                                }
                            ]
                        },
                        {
                            "className": "column-left",
                            "style": "display:none",
                            "elements": [
                                {
                                    "name": "patientId",
                                    "id": "patientId",
                                    "type": "newlabel",
                                    "className": "",
                                    "title": ""
                                }
                            ]
                        },
                        {
                            "className": "column-right",
                            "data": [
                                {
                                    "className": "column-left",
                                    "id": "viewPatientType",
                                    "elements": [
                                        {
                                            "name": "patientTypelbl",
                                            "id": "patientTypelbl",
                                            "type": "newlabel",
                                            "title": "Patient Type",
                                            "className": "newBox "
                                        }
                                    ]
                                },
                                {
                                    "className": "column-left",
                                    "style": "display:none",
                                    "id": "editPatientType",
                                    "elements": [
                                        {
                                            "name": "patientType",
                                            "id": "patientType",
                                            "type": "radio",
                                            "title": "Patient Type",
                                            "value": [
                                                {
                                                    "title": "InPatient",
                                                    "value": "InPatient",
                                                    "checked": "true"
                                                },
                                                {
                                                    "title": "OutPatient",
                                                    "value": "OutPatient"
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "className": "column-right",
                                    "elements": [
                                        {
                                            "name": "date",
                                            "id": "date",
											"readonly":"readonly",
                                            "type": "text",
                                            "className": "newBox",
                                            "title": "Admitted Date"
                                        }
                                    ]
                                }
                              
                            ]
                        }
                    ]
                },
                {
                    "className": "five_sixth",
                    "data": [
                        {
                            "className": "column-left",
                            "elements": [
                                {
                                    "name": "case",
                                    "id": "case",
									"readonly":"readonly",
                                    "tabIndex": 1,
                                    "type": "text",
                                    "className": "newBox capital",
                                    "title": "Case"
                                }
                            ]
                        },
                        {
                            "className": "column-right",
                            "id": "viewDepartment",
                            "elements": [
                                {
                                    "name": "department",
                                    "id": "department",
                                    "tabIndex": 2,
                                    "type": "newlabel",
                                    "className": " newBox ",
                                    "title": "Department"
                                }
                            ]
                        },
                        {
                            "className": "column-right",
                            "id": "editDepartment",
                            "style": "display:none",
                            "elements": [
                                {
                                    "name": "department",
                                    "id": "department",
                                    "tabIndex": 2,
                                    "type": "select",
                                    "className": "select",
                                    "title": "Department"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "className": "sep-div-view",
            "id": "question",
            "style": "display: none",
            "data": [
                {
                    "className": "five_sixth",
                    "id": "questionnaire"
                }
            ]
        },
        {
            "className": "sep-div-view",
            "style": "display: none",
            "id": "generalDetails",
            "data": [
                {
                    "className": "five_sixth",
                    "data": [
                        {
                            "className": "column-left",
                            "data": [
                                {
                                    "className": "column-left",
                                    "elements": [
                                        {
                                            "name": "height",
                                            "id": "height",
											"readonly":"readonly",
                                            "type": "text",
											 "tabIndex": 3,
                                            "className": "newBox",
                                            "title": "Height (meters)"
                                        }
                                    ]
                                },
                                {
                                    "className": "column-right",
                                    "elements": [
                                        {
                                            "name": "weight",
                                            "id": "weight",
											"readonly":"readonly",
                                            "tabIndex": 4,
                                            "type": "text",
                                            "className": "newBox",
                                            "title": "Weight (Kg)"
                                        }
                                    ]
                                }
                            ] 
                        },
                        {
                            "className": "column-right",
                            "data": [
                                {
                                    "className": "column-left",
                                    "elements": [
                                        {
                                            "name": "bmi",
                                            "id": "bmi",
											"readonly":"readonly",
                                            "type": "newlabel",
                                            "title": "BMI",
                                            "className": "newBox"
                                        }
                                    ]
                                },
                                {
                                    "className": "column-right",
                                    "elements": [
                                        {
                                            "name": "hb",
                                            "id": "hb",
											"readonly":"readonly",
                                            "tabIndex": 5,
                                            "type": "text",
                                            "className": "newBox",
                                            "title": "Hb"
                                        }
                                    ]
                                }
                            ]
                        },
                        {
                            "className": "five_sixth",
                            "elements": [
                                {
                                    "name": "description",
                                    "id": "description",
									"readonly":"readonly",
                                    "tabIndex": 6,
                                    "type": "textarea",
                                    "style": "height: 100px;padding: none;float: left;",
                                    "className": "newBox",
                                    "title": "Description",
                                    "required": true
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "className": "sep-div-view",
            "style": "margin-bottom: 5px",
            "data": [
                {
                    "className": "five_sixth",
                    "elements": [
                        {
                            "type": "button",
                            "content": {
                                "buttons": [
                                    {
                                        "name": "btnCaseEdit",
                                        "id": "btnCaseEdit",
                                        "title": "Edit",
                                        "type": "button",
                                        "className": "stdbtn btn_black",
                                        "style": "opacity: 1;margin: 4px;float: right"
                                    },
                                    {
                                        "name": "btnCaseSave",
                                        "id": "btnCaseSave",
                                        "title": "Done",
                                        "type": "button",
                                        "className": "stdbtn btn_black",
                                        "style": "opacity: 1;margin: 4px;float: right;display: none;"
                                    },
                                    {
                                        "name": "btnCaseCancel",
                                        "id": "btnCaseCancel",
                                        "title": "Cancel",
                                        "type": "button",
                                        "className": "stdbtn",
                                        "style": "opacity: 1;margin: 4px;float: right;display: none;"
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    ]
}