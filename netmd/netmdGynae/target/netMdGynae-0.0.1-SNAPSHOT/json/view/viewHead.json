{
    "name": "viewHead",
    "id": "viewHead",
    "content": [
        {
            "className": "sep-div-view",
            "data": [
                {
                    "className": "column-left",
                    "data": [
                        {
                            "className": "column-one-fourth",
                            "elements": [
                                {
                                    "name": "id",
                                    "id": "id",
                                    "type": "newlabel",
                                    "style": "margin-left:6%",
                                    "title": "Id"
                                }
                            ]
                        },
                        {
                            "className": "column-three-fourth",
                            "elements": [
                                {
                                    "name": "headName",
                                    "id": "headName",
                                    "type": "text",
                                    "readonly": "readonly",
                                    "className": "newBox large",
                                    "title": "Head"
                                }
                            ]
                        }
                    ]
                },
                {
                    "className": "column-right",
                    "data": [
                        
						{
                            "className": "column-left",
                            "elements": [
                                {
                                    "name": "parentHeadname",
                                    "id": "parentHeadname",
                                    "type": "newlabel",
                                    "readonly": "readonly",
                                    "title": "Parent Head"
                                }
                            ]
                        }
                      
                    ]
                },
				{
                    "className": "five_sixth",
                    "data": [
					{
								"className": "column-left",
								"id":"viewHeadType",
								"elements": [
									{
										"name": "lblHeadType",
										"id": "lblHeadType",
										"type": "newlabel",
										"title": "Head Type"
									}
								]	
								},
								{
								"className": "column-left",
								"id":"editHeadType",
								"style":"display:none",
								"elements": [
									{
										"name": "HeadType",
										"id": "HeadType",
										"type": "select",
										"className": "select",
										"style":"width:100%",
										"readonly": "readonly",
										"title": "Select Head Type"
									}
								]	
								},
                                {
                                "className": "column-right",
                                "elements": [
										{
											"name": "description",
											"id": "description",
											"type": "textarea",
											"readonly": "readonly",
											"className": "newBox large",
											"title": "Description"
										}
                                    ]
                                }
                            ]
                 }
            ]
        },
        {
            "className": "sep-div-view",
            "style": "margin-bottom:5px",
            "data": [
                {
                    "className": "five_sixth",
                    "elements": [
                        {
                            "type": "button",
                            "content": {
                                "buttons": [
                                    {
                                        "name": "btnHeadEdit",
                                        "title": "Edit",
                                        "type": "button",
                                        "className": "stdbtn btn_black",
                                        "style": "opacity: 1; float:left;"
                                    },
                                    {
                                        "name": "btnHeadSave",
                                        "title": "Save",
                                        "type": "button",
                                        "className": "stdbtn btn_black",
                                        "style": "opacity:1;display:none;"
                                    },
                                    {
                                        "name": "btnHeadCancel",
                                        "title": "Cancel",
                                        "type": "button",
                                        "className": "stdbtn",
                                        "style": "opacity:1;display:none;"
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    ]
}